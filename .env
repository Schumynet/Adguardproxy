# Porta del server (Render imposta automaticamente la variabile PORT)
PORT=3000

# Server AdGuard DNS
ADGUARD_DNS_PRIMARY=94.140.14.14
ADGUARD_DNS_SECONDARY=94.140.15.15

# Timeout per le richieste (in millisecondi)
REQUEST_TIMEOUT=30000

# Domini consentiti per il proxy (separati da virgola)
ALLOWED_DOMAINS=vixsrc.to,example.com

# Log level (debug, info, warn, error)
LOG_LEVEL=info

# Abilita caching DNS (true/false)
DNS_CACHING_ENABLED=true
DNS_CACHE_TTL=3600000